@model List<CalculatorFP.ViewModels.CandidateListViewModel>


@{
    ViewBag.Title = "Chart";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Chart</h2>
<div>
    @{
        var chart = new Chart(width: 800, height: 600)
       .AddSeries("Default", chartType: "Bar",
            xValue: Model, xField: "CandidateName",
            yValues: Model, yFields: "CandidateCount")
        .Write();
    }

</div>



